    "arctic": "^1.9.2",
    "better-auth": "^1.4.10",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.6.3",
    "hugeicons-react": "^0.3.0",
    "lucide-react": "^0.562.0",
    "postgres": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.71.0",
    "resend": "^4.0.0",
    "tailwind-merge": "^2.5.2",
    "zod": "^4.3.5"
