    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "bun@1.1.0"
